<?xml version='1.0' encoding='UTF-8'?>
 
<!-- =========== FULL CONFIGURATION FILE ================================== -->
 
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
   
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>exito</from-outcome>
            <to-view-id>/InventoryPOSPresentacion/faces/insumosVista/ingresarInsumo.xhtml</to-view-id>
        </navigation-case>    
    </navigation-rule>
    <navigation-case>
        <from-outcome>fallo</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
   
   
<managed-bean>
    <managed-bean-name>usuario</managed-bean-name>>
    <managed-bean-class>beans.UsuarioVistaBean</managed-bean-class>
    
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>    
 
<managed-bean>
    <managed-bean-name>insumo</managed-bean-name>>
    <managed-bean-class>beans.InsumoVistaBean</managed-bean-class>
    
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean> 
</faces-config>